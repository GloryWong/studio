{
  "name": "@gloxy/studio",
  "description": "Manage local projects and demos in one place instantly and smoothly.",
  "version": "0.0.0",
  "author": "Glory Wong Zhaohui @GloryWong",
  "bin": {
    "gs": "./bin/run"
  },
  "bugs": "https://github.com/GloryWong/studio/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.19",
    "@types/inquirer": "^7.3.1",
    "@types/listr": "^0.14.3",
    "@types/luxon": "^1.27.1",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.0.0",
    "@types/node-localstorage": "^1.3.0",
    "chai": "^4.3.4",
    "commitlint": "^12.1.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "husky": "^7.0.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "recursive-copy-cli": "^1.0.17",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/GloryWong/studio",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "gs",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "GloryWong/studio",
  "scripts": {
    "dev": "rm -rf lib && tsc --sourceMap true -b -w",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@gloxy/unilog": "^1.1.5",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "boxen": "^5.0.1",
    "cfonts": "^2.9.3",
    "cli-columns": "^3.1.2",
    "conf": "^10.0.1",
    "dotenv-flow": "^3.2.0",
    "execa": "^5.1.1",
    "fuse.js": "^6.4.6",
    "inquirer": "^8.1.0",
    "listr": "^0.14.3",
    "lodash": "^4.17.21",
    "luxon": "^1.27.0",
    "mkdirp": "^1.0.4",
    "node-localstorage": "^2.2.1",
    "ora": "^5.4.0",
    "recursive-copy": "^2.0.13",
    "tslib": "^2.3.0",
    "uid": "^2.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
